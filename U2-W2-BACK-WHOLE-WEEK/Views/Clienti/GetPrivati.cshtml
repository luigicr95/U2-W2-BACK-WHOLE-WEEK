@model IEnumerable<U2_W2_BACK_WHOLE_WEEK.Models.Cliente>

@{
    ViewBag.Title = "GetPrivati";
}

<h2>GetPrivati</h2>

<p>
    @Html.ActionLink("Create New", "AddPrivati")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Nome)
